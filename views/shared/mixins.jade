mixin mainInclude(pageScript)
    script(src='/javascripts/lib/require.js')

    script
        if(typeof(KEENTOUR)==="undefined"){KEENTOUR={};}
         //Load global code that includes config, then load the app
         //logic for this page. Do the require calls here instead of
         //a separate file so after a build there are only 2 HTTP
         //requests instead of three.
         require(['/javascripts/global.js'], function () {
             require(['#{pageScript}']);
         });


